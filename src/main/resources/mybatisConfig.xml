<configuration>
		<settings>
		<!-- 널허용 : 매퍼에서 값을 가져올때 null가능 -->  
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<!-- 매퍼 resultType의 알리아스 --> 
	<typeAliases>
		<typeAlias type="game.site.wep.dto.User" alias="User"/>	
		<typeAlias type="game.site.wep.dto.Gameboard" alias="Gameboard"/>
		<typeAlias type="game.site.wep.dto.Gamese" alias="Gamese"/>
		<typeAlias type="game.site.wep.dto.Reply" alias="Reply"/>
		<typeAlias type="game.site.wep.dto.Trade" alias="Trade"/>
		<typeAlias type="game.site.wep.dto.GameFile" alias="GameFile"/>
		
	</typeAliases>

</configuration>  